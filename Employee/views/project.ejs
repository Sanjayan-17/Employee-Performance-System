    <script type="text/JavaScript">
        let a=1;
        function createNewElement() {
            // First create a DIV element.
            var txtNewInputBox = document.createElement('div');
        
            // Then add the content (a new input box) of the element.
            txtNewInputBox.innerHTML = "<input type='text' id='"+a+"'>";
        
            // Finally put it where it is supposed to appear.
            document.getElementById("newElementId").appendChild(txtNewInputBox);
            a=a+1;
        }
        </script>
</head>
<body>
    <br><br>
    <center>
    <div class="form">
        <form action = "/project" enctype="multipart/form-data" method = "post">
            <h1 class="reg" style="color:black">Project</h1>
            <label>Your Name <span style="color:red">*</span></label>
            <input type="text" id="name" name = "name" placeholder="Enter your  name"/>
            <label>Project Name <span style="color:red">*</span></label>
            <input type="text" id="projname" name = "projname" placeholder="Enter your project name"/>
            <label>Project Folder(upload as a zip file)</label>
            <input type="file" id="SRS" name = "SRS" placeholder="Enter the Project SRS"/>
            <label>Project Description</label>
            
            <textarea  id="projdetails" name = "projdesc"rows="6" cols="70"></textarea>
            <label>Project Link<span style="color:red">*</span></label>
            <input type="text" id="plink" name = "projlink" placeholder="Example:Github Link"/>
            <p>        </p>
            <button type="submit">Submit</button>
        </form>
    </div>
    </center>
</body>
